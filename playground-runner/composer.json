{
	"require": {
		"php": "~7.2",
		"bref/bref": "^0.5",
		"jean85/pretty-package-versions": "^1.2",
		"nette/neon": "^3.0",
		"nikic/php-parser": "^4.3.0",
		"phpstan/phpstan": "^0.12",
		"phpstan/phpstan-strict-rules": "^0.12.99",
		"sentry/sentry": "^1.9"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"phpunit/phpunit": "^8.0"
	}
}
