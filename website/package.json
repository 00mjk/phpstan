{
	"name": "phpstan-website",
	"version": "1.0.0",
	"license": "MIT",
	"dependencies": {
		"@tailwindcss/ui": "^0.1.3",
		"codemirror": "^5.52.2",
		"docsearch.js": "2",
		"jquery": "^3.4.1",
		"knockout": "^3.5.1",
		"prism-themes": "^1.4.0"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.10.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
		"@fullhuman/postcss-purgecss": "^2.1.0",
		"@tailwindcss/custom-forms": "^0.2.1",
		"@types/jquery": "^3.3.34",
		"@types/knockout": "^3.4.67",
		"autoprefixer": "^9.7.5",
		"markdown-it-anchor": "^5.2.7",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-clean-dist": "^0.0.6",
		"postcss-modules": "^1.5.0",
		"tailwindcss": "^1.2.0",
		"tslint": "^6.1.0",
		"typescript": "^3.8.3"
	},
	"scripts": {
		"check": "tsc -p tsconfig.json --noEmit && tslint -p tsconfig.json -c tslint.json",
		"watch:parcel": "parcel tmp/*.html tmp/**/*.html tmp/*.xml tmp/robots.txt",
		"watch:11ty": "eleventy --watch",
		"build:parcel": "parcel build tmp/*.html tmp/**/*.html tmp/*.xml tmp/robots.txt --no-source-maps",
		"build:11ty": "eleventy",
		"watch": "run-p watch:*",
		"build": "yarn run build:11ty && yarn run build:parcel"
	}
}
