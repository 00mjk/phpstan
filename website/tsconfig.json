{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"strict": true,
		"lib": ["dom", "es2015"],
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"js/**/*"
	]
}
